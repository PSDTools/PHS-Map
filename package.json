  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "jquery": "^3.7.0",
    "pathfinding": "^0.4.18",
    "vite-plugin-webfont-dl": "^3.7.6",
    "workbox-build": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-precaching": "^7.0.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.16",
    "@types/pathfinding": "^0.0.6",
    "eslint": "^8.47.0",
    "eslint-config-sheriff": "^14.4.0",
    "eslint-define-config": "^1.20.0",
    "prettier": "^3.0.2",
    "typescript": "^5.2.2",
    "vite": "^4.3.9",
    "vite-plugin-pwa": "^0.16.4"
  },
  "scripts": {
    "build": "bun run typecheck && bun run lint && bun run format:check && bun run bundle",
    "dev": "bun --bun x vite --open",
    "bundle": "bun --bun x vite build",
    "typecheck": "bun run typecheck:src && bun run typecheck:sw && bun run typecheck:extra",
    "typecheck:src": "bun --bun x tsc",
    "typecheck:sw": "bun --bun x tsc -p tsconfig.sw.json",
    "typecheck:extra": "bun --bun x tsc -p tsconfig.eslint.json",
    "format": "bun --bun x prettier . --write",
    "format:check": "bun --bun x prettier . --check",
    "lint": "bun x eslint ."
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
